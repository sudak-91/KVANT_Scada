        <Button x:Name="Shield_Auto_On" Content="Вкл.Автоматический режим" HorizontalAlignment="Left" Height="57" Margin="12,137,0,0" VerticalAlignment="Top" Width="157" Click="Shield_Auto_On_Click"/>
        <Button x:Name="Shield_Auto_OFF" Content="Выкл.Автоматический режим" HorizontalAlignment="Left" Height="57" Margin="13,207,0,0" VerticalAlignment="Top" Width="157" Click="Shield_Auto_OFF_Click"/>
        <Button x:Name="Shield_Open" Content="Открыть" HorizontalAlignment="Left" Height="57" Margin="199,135,0,0" VerticalAlignment="Top" Width="157" Click="Shield_Open_Click"/>
        <Button x:Name="Shield_close" Content="Закрыть" HorizontalAlignment="Left" Height="57" Margin="199,205,0,0" VerticalAlignment="Top" Width="157" Click="Shield_close_Click"/>

    </Grid>
</Window>
