<Window x:Class="KVANT_Scada.GUI.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KVANT_Scada.GUI"
        mc:Ignorable="d"
        Title="Window1" Height="389.691" Width="373.196" Topmost="True">
    <Grid>
        <Ellipse x:Name="Shield_Auto_ind" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="19" Margin="20,26,0,0" Stroke="Black" VerticalAlignment="Top" Width="19"/>
